    "@angular-devkit/core": "^0.0.29",
    "@angular-devkit/schematics": "0.0.52",
    "@angular/cli": "1.7.1",
    "@angular/compiler-cli": "5.2.6",
    "@nrwl/schematics": "0.8.0",
    "@angular/language-service": "5.2.6",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
