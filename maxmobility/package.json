{
  "name": "maxmobility",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start.web": "ng serve --app=web",
    "start.eval.mobile.android": "npm run sema",
    "sema": "npm i && cd apps/eval-mobile && tns run android",
    "debug.eval.mobile.android": "npm run dema",
    "dema": "npm i && cd apps/eval-mobile && tns debug android",
    "start.eval.mobile.ios": "npm run semi",
    "semi": "npm i && cd apps/eval-mobile && tns run ios",
    "debug.eval.mobile.ios": "npm run demi",
    "demi": "npm i && cd apps/eval-mobile && tns debug ios",
    "sema-bluetooth": "npm i && cd apps/eval-mobile && tns run android --syncAllFiles",
    "semi-bluetooth": "npm i && cd apps/eval-mobile && tns run ios --syncAllFiles",
    "build": "ng build",
    "bundle-android": "cd apps/eval-mobile && tns run android --bundle --env.aot --env.uglify",
    "bundle-ios": "cd apps/eval-mobile && tns run ios --bundle --env.aot --env.uglify",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "affected:apps": "./node_modules/.bin/nx affected apps",
    "affected:build": "./node_modules/.bin/nx affected build",
    "affected:e2e": "./node_modules/.bin/nx affected e2e",
    "format": "./node_modules/.bin/nx format write",
    "format:write": "./node_modules/.bin/nx format write",
    "format:check": "./node_modules/.bin/nx format check",
    "update": "./node_modules/.bin/nx update",
    "update:check": "./node_modules/.bin/nx update check",
    "update:skip": "./node_modules/.bin/nx update skip",
    "postinstall": "./node_modules/.bin/nx postinstall",
    "clean": "rm -rf hooks node_modules package-lock.json && npm i",
    "clean.eval.mobile": "cd apps/eval-mobile && rm -rf hooks node_modules platforms package-lock.json && npm i",
    "clean.shared": "npx rimraf -- libs/**/*.js && npx rimraf -- libs/**/*.map && npx rimraf -- libs/**/*.d.ts && npx rimraf -- libs/**/*.ngfactory.ts && npx rimraf -- libs/**/*.ngsummary.json && npx rimraf -- libs/**/*.metadata.json && npx rimraf -- xplat/**/*.js && npx rimraf -- xplat/**/*.map && npx rimraf -- xplat/**/*.d.ts && npx rimraf -- xplat/**/*.ngfactory.ts && npx rimraf -- xplat/**/*.ngsummary.json && npx rimraf -- xplat/**/*.metadata.json",
    "nuke": "npm run clean && npm run clean.eval.mobile && npm run clean.shared",
    "nuki": "npm run nuke && npm i",
    "ns-setup": "npm uninstall -g nativescript && npm install -g nativescript",
    "ios-release": "npm run nuki && cd apps/eval-mobile && tns build ios --release",
    "android-release": "node ./apps/eval-mobile/tools/android-release.js",
    "precommit": "lint-staged",
    "profile-android-layouts": "adb shell dumpsys gfxinfo com.permobil.smartevaluation > layout-profile.txt",
    "android-framestats": "adb shell dumpsys gfxinfo com.permobil.smartevaluation framestats > framestats.txt"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "5.2.1",
    "@angular/material": "5.2.1",
    "@angular/platform-server": "~6.0.0",
    "@angular/animations": "~6.1.0",
    "@angular/common": "~6.1.0",
    "@angular/compiler": "~6.1.7",
    "@angular/core": "~6.1.0",
    "@angular/forms": "~6.1.0",
    "@angular/http": "~6.1.0",
    "@angular/platform-browser": "~6.1.0",
    "@angular/platform-browser-dynamic": "~6.1.0",
    "@angular/router": "~6.1.0",
    "@ng-bootstrap/ng-bootstrap": "~2.2.2",
    "@ngrx/effects": "~6.1.0",
    "@ngrx/entity": "~6.1.0",
    "@ngrx/router-store": "~6.1.0",
    "@ngrx/store": "~6.1.0",
    "@ngrx/store-devtools": "~6.1.0",
    "@ngx-translate/core": "~10.0.2",
    "@ngx-translate/http-loader": "~3.0.1",
    "@nrwl/nx": "~6.2.0",
    "buffer": "^5.1.0",
    "colors": "^1.2.1",
    "core-js": "^2.5.4",
    "email-validator": "^2.0.4",
    "kinvey-nativescript-sdk": "^3.11.7",
    "nativescript-angular": "^6.1.0",
    "nativescript-intl": "^3.0.0",
    "nativescript-barcodescanner": "~2.7.8",
    "nativescript-carousel": "~4.0.2",
    "nativescript-drop-down": "~4.0.1",
    "nativescript-feedback": "^1.2.0",
    "nativescript-gradient": "^2.0.1",
    "nativescript-iqkeyboardmanager": "~1.3.0",
    "nativescript-loading-indicator": "~2.4.0",
    "nativescript-localstorage": "~1.1.5",
    "nativescript-master-technology": "^1.1.1",
    "nativescript-ngx-fonticon": "~4.2.0",
    "nativescript-permissions": "~1.2.3",
    "nativescript-snackbar": "~3.1.0",
    "nativescript-theme-core": "~1.0.4",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "tns-core-modules": "^4.2.0",
    "underscore": "^1.9.1",
    "zone.js": "^0.8.26"
  },
  "lint-staged": {
    "*.{ts, js, json, css, scss, md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.1",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "~6.1.7",
    "@angular/language-service": "~6.0.0",
    "@ngrx/schematics": "~6.0.1",
    "@nrwl/schematics": "~6.1.1",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nativescript-worker-loader": "~0.9.0",
    "node-fetch": "~2.0.0",
    "node-sass": "^4.9.2",
    "prettier": "^1.14.2",
    "protractor": "~5.3.0",
    "tns-platform-declarations": "^4.2.0",
    "husky": "^0.14.3",
    "prompt": "^1.0.0",
    "lint-staged": "^7.2.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.10.0",
    "typescript": "~2.8.2"
  }
}
